        <string notr="true">QLabel{border-style:none;color:rgb(33, 33, 33);background-color: rgba(255, 255, 255, 0);}
QComboBox {
    border: 2px solid rgb(169, 169, 169); /* border: 宽度 线类型 颜色 */
    height:25px;
    /*background-color: rgba(0, 150, 255, 150);*/
    background-color: rgba(214, 214, 214,100);
    border-radius:10px;
}

/*下拉框的样式*/
QComboBox QAbstractItemView
{
    outline: 0px solid gray;  /*取消选中虚线*/
    border: 1px solid rgb(31, 156, 255);
    color: rgb(66, 66, 66);
    background-color: rgb(255, 255, 255);
    selection-background-color: rgb(0, 150, 255);
    border-radius:5px;
    height:35px;
    border:None;
}
/*选中每一项的字体颜色和背景颜色*/
QComboBox::item:selected 
{
    color: rgb(255, 255, 255);
    background-color: rgb(0, 150, 255);
    border-radius:5px;
}
/*右边*/
QComboBox::drop-down{border: 2px solid rgba(0, 150, 255,0);}
QComboBox::down-arrow
{
    image: url(:/img/back_blue_down.png);
    width: 25px;
    height: 35px;
    right:6px;
    border-left: 2px solid rgb(192, 192, 192);
    border-right: 1px solid rgba(214, 214, 214,0);
}
QComboBox::down-arrow:on
{
    image: url(:/img/back_blue_up.png);
    width: 25px;
    height: 35px;
    right:6px;
    border-left: 2px solid rgb(192, 192, 192);
    border-right: 1px solid rgba(214, 214, 214,0);
}
/*QComboBox::down-arrow::hover{}*

/* QComboBox中的垂直滚动条 */
QComboBox QAbstractScrollArea QScrollBar:vertical {
    width: 10px;
    height: 8px;
    background-color: rgb(255, 64, 255);   /* 空白区域的背景色*/
    border-style:none;
}

QComboBox QAbstractScrollArea QScrollBar::handle:vertical {
    border-radius: 5px;   /* 圆角 */
    background: rgb(160,160,160);   /* 小方块的背景色深灰lightblue */
}

QComboBox QAbstractScrollArea QScrollBar::handle:vertical:hover {
    background: rgb(255, 255, 255);   /* 越过小方块的背景色*/
